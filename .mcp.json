{
  "mcpServers": {
    "serena": {
      "type": "stdio",
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena-mcp-server",
        "--context",
        "ide-assistant"
      ]
    },
    "surrealdb-mcp": {
      "command": "docker",
      "args": [
        "run",
        "--rm",
        "-i",
        "--pull",
        "always",
        "surrealdb/surrealmcp:latest",
        "start"
      ],
      "disabled": false,
      "autoApprove": []
    },
    "vantage-mcp": {
      "type": "stdio",
      "command": "./target/release/vantagemcp",
      "args": [
        "--web",
        "--web-port",
        "8080"
      ]
    }
  }
}